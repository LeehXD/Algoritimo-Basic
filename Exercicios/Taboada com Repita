algoritmo "semnome"

var
cont, numero: inteiro

inicio


////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

escreval ("--------------------------------------")
escreval ("DIGITE UM NUMERO E VEJA SUA TABUADA =>")
escreval ("--------------------------------------")
leia (numero)

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

cont <- 1

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

escreval ("--------------------------------------")

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

repita

   escreval (cont," X",numero ," =",cont*numero)

   cont <- cont + 1

ate (cont > 10)

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

escreval ("--------------------------------------")

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

fimalgoritmo
